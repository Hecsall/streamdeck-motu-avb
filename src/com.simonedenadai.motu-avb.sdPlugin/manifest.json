{
	"SDKVersion": 2,
	"Author": "Simone De Nadai",
	"CodePath": "app.html",
	"Description": "Remote control for MOTU AVB Audio Interfaces",
	"Name": "MOTU AVB",
	"Icon": "assets/icons/pluginIcon",
	"URL": "https://github.com/Hecsall/streamdeck-motu-avb",
	"Version": "1.0.0",
	"Software": {
		"MinimumVersion": "5.0"
	},
	"OS": [
		{
			"Platform": "mac",
			"MinimumVersion": "10.11"
		},
		{
			"Platform": "windows",
			"MinimumVersion": "10"
		}
	],
	"Category": "MOTU AVB",
	"CategoryIcon": "images/category/categoryIcon",
	"Actions": [
		{
			"Icon": "images/actions/toggle",
			"Name": "Toggle On/Off",
			"Controllers": ["Keypad"],
			"States": [
				{
					"Image": "images/icons/toggleOn",
					"Name": "Toggle On"
				},
				{
					"Image": "images/icons/toggleOff",
					"Name": "Toggle Off"
				}
			],
			"Tooltip": "Toggle On/Off a channel",
			"UUID": "com.simonedenadai.motu-avb.toggleonoff",
			"PropertyInspectorPath": "actions/inspector.html"
		},
		{
			"Icon": "images/actions/toggle",
			"Name": "Toggle Values",
			"Controllers": ["Keypad"],
			"States": [
				{
					"Image": "images/icons/toggleOn",
					"Name": "Toggle On"
				},
				{
					"Image": "images/icons/toggleOff",
					"Name": "Toggle Off"
				}
			],
			"Tooltip": "Toggle between 2 values in a channel",
			"UUID": "com.simonedenadai.motu-avb.togglevalues",
			"PropertyInspectorPath": "actions/inspector.html"
		},
		{
			"Icon": "images/actions/setValue",
			"Name": "Set Value",
			"Controllers": ["Keypad"],
			"States": [
				{
					"Image": "images/icons/setValue",
					"Name": "Set value"
				}
			],
			"Tooltip": "Sets a value in a channel",
			"UUID": "com.simonedenadai.motu-avb.setvalue",
			"PropertyInspectorPath": "actions/inspector.html"
		}
	]
}
